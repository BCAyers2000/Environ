 &ENVIRON
    verbose = 1,
    environ_thr = 0.1,
    system_axis = 3,
    system_dim = 2,
    temperature = 300,
    sc_permittivity = 11,
    sc_carrier_density = 1.D18,   
/
 &BOUNDARY
    sc_spread = 0.001,
    sc_distance = 10.2137
    electrolyte_spread = 0.001,
    electrolyte_distance = 20.2137,
    electrolyte_mode = 'system',
/
 &ELECTROSTATIC
    problem = 'poisson'
    solver = 'direct'
    pbc_correction = 'ms'
    pbc_dim = 2                 
    pbc_axis = 3   
    tol = 1e-11,
/
